# Environment
ENVIRONMENT=production
STACK_NAME=MyProdStack
PROJECT_NAME="example project"

# AWS Configuration
AWS_ACCESS_KEY_ID=your-production-access-key
AWS_SECRET_ACCESS_KEY=your-production-secret-key
AWS_DEFAULT_REGION=ap-northeast-1

# Geth Configuration (Production)
GETH_ADDRESS=your-production-geth-address
GETH_PRIVATE_KEY=your-production-private-key
GETH_PASSWORD=production-secure-password
GETH_INITIAL_BALANCE=500000000000000000000000
GETH_CHAIN_ID=12345

# ドメイン設定
DOMAIN_NAME=web3.example.com
EMAIL=admin@example.com

# Elastic IP設定
USE_ELASTIC_IP=false
# 事前に取得したElastic IPのAllocation ID（例: eipalloc-1234567890abcdef0）
ELASTIC_IP_ALLOCATION_ID=  

# SSH-KEY作成
# EC2_KEY_NAME=production-key

# Application Settings  
APP_NAME=my-cdk-app-prod

# Network Settings
VPC_CIDR=10.1.0.0/16

# Database Settings
DB_INSTANCE_CLASS=t3.small
BACKUP_RETENTION_DAYS=30

# Lambda Settings
LAMBDA_MEMORY=512
LAMBDA_TIMEOUT=30

# Monitoring
LOG_LEVEL=warn
ENABLE_XRAY=true

# Security
ENABLE_VPC_FLOW_LOGS=true
ENABLE_CLOUDTRAIL=true

# Discord Bot Configuration
# Discord Developer Portal から取得した値を設定
# https://discord.com/developers/applications でアプリケーションを作成後

# DISCORD-BOT
DISCORD_PUBLIC_KEY=your_discord_bot_public_key_here
DISCORD_APP_ID=your_discord_application_id_here
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_GUILD_ID=your_discord_server_id

# NFT Configuration                                           
NFT_CONTRACT_ADDRESS=0x000000000000000000000000000000000000dEaD
NFT_CHAIN_ID=12345
NFT_RPC_URL=https://web3.example.com/geth
API_BASE_URL=https://example.execute-api.ap-northeast-1.amazonaws.com
